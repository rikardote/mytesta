<p id="notice"><%= notice %></p>

<h1>Listing Adscriptions</h1>

<table>
  <thead>
    <tr>
      <th>Cod</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adscriptions.each do |adscription| %>
      <tr>
        <td><%= adscription.cod %></td>
        <td><%= adscription.name %></td>
        <td><%= link_to 'Show', adscription %></td>
        <td><%= link_to 'Edit', edit_adscription_path(adscription) %></td>
        <td><%= link_to 'Destroy', adscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Adscription', new_adscription_path %>
